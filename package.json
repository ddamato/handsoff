    "url": "https://github.com/ddamato/handsoff/issues"
  },
  "homepage": "https://github.com/ddamato/handsoff#readme",
  "devDependencies": {
    "concurrently": "^5.0.2",
    "esm": "^3.2.25",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.6",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "koa-static-server": "^1.4.0",
    "make-runnable": "^1.3.6",
    "markdown-it": "^12.3.2",
    "nodemon": "^2.0.2",
